@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .

<http://ssj71.github.io/infamousPlugins/plugs.html#hip2b>
        a lv2:Plugin, lv2:DistortionPlugin ;
        doap:name "the infamous Hip2B" ;
        doap:license <http://opensource.org/licenses/GPL-2.0> ;
        doap:description "A square wave distortion/destroyer" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://ssj71.github.io/infamousPlugins> ;
                foaf:mbox <mailto:ssjackson71@gmail.com> ;
        ] ;
        mod:brand "infamous" ;
        mod:label "hip2b" ;
        modgui:gui [
            modgui:resourcesDirectory <modgui> ;
            modgui:iconTemplate <modgui/hip2b.html> ;
            modgui:stylesheet <modgui/hip2b.css> ;
            modgui:screenshot <modgui/screenshot-hip2b.png> ;
            modgui:thumbnail <modgui/thumbnail-hip2b.png> ;
            modgui:brand "infamous" ;
            modgui:label "hip2b" ;
        ] ;
        lv2:minorVersion 0 ;
        lv2:microVersion 1 ;
        rdfs:comment "This is a destroyer or extreme distortion which is actually a switching function with hysteresis. Basically it makes any signal sound like a square wave, and is capable of good glitch sounds when the octave is lowered." ;
        lv2:optionalFeature lv2:hardRTCapable ;
        ui:ui <http://ssj71.github.io/infamousPlugins/plugs.html#hip2b_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "LATENCY" ;
                lv2:name "Latency" ;
                lv2:minimum 0 ;
                lv2:maximum 100000 ;
                lv2:portProperty lv2:reportsLatency, lv2:integer, pprops:notOnGUI;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "UP" ;
                lv2:name "Up" ;
                lv2:default 0.02 ;
                lv2:minimum -0.5 ;
                lv2:maximum 0.5 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "DOWN" ;
                lv2:name "Down" ;
                lv2:default 0.02 ;
                lv2:minimum -0.5 ;
                lv2:maximum 0.5 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "OCTAVE" ;
                lv2:name "Octave" ;
                lv2:default 0 ;
                lv2:minimum -2 ;
                lv2:maximum 0 ;
                lv2:portProperty lv2:integer;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "IN_GAIN" ;
                lv2:name "Input Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 2.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "WETDRY" ;
                lv2:name "Wet/Dry Mix" ;
                lv2:default 0.7 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "OUT_GAIN" ;
                lv2:name "Output Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 2.0 ;
        ] .

